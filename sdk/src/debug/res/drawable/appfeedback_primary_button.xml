<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="false">
        <shape>
            <solid
                android:color="@color/appfeedback_white"/>
            <stroke
                android:color="@color/appfeedback_primary"
                android:width="0.5dp"/>
        </shape>
    </item>
    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="@color/appfeedback_super_light_gray"/>
            <stroke
                android:color="@color/appfeedback_primary"
                android:width="0.5dp"/>
        </shape>
    </item>
</selector>
